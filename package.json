{
  "name": "postgres-playground",
  "private": true,
  "version": "0.0.0",
  "author": "Daniel Gray<danielfgray@gmail.com>",
  "License": "MIT",
  "type": "module",
  "scripts": {
    "dev:client": "vite --clearScreen false",
    "dev:server": "bun --watch --no-clear-screen server/index.ts",
    "pre:dev": "bun ./scripts/dbStart.mjs",
    "dev": "run-p dev:client dev:server db:watch",
    "test:watch": "bun test --watch --no-clear-screen",
    "test:units": "bun test",
    "test:e2e": "cypress run --headless",
    "test:browser": "cypress open --e2e",
    "test": "run-s test:units",
    "format": "prettier --write src server scripts cypress *.html *.json .ts .js .cjs",
    "lint:js": "eslint --ext --ignore-path .gitignore --cache-file node_modules/.cache/eslintcache .js,.jsx,.ts,.tsx .",
    "lint": "run-s lint:js",
    "typecheck:watch": "tsc --noEmit --pretty --watch --preserveWatchOutput",
    "build:tsc": "tsc",
    "build:vite": "vite build",
    "build": "run-s build:vite",
    "preview": "vite preview",
    "env:init": "bun scripts/generateEnvFile.mjs",
    "gm": "bun -b graphile-migrate",
    "db:init": "bun scripts/setup.mjs",
    "db:start": "docker-compose up -d db",
    "db:latest": "bun -b graphile-migrate watch --once",
    "db:watch": "bun -b graphile-migrate watch",
    "db:reset": "bun -b graphile-migrate reset --erase",
    "db:stop": "docker-compose stop db",
    "db:exec": "docker-compose exec db",
    "db:setup": "run-s -s db:start db:init db:reset db:latest",
    "predev": "run-s -s db:start",
    "setup": "NOCONFIRM=1 run-s -s env:init db:setup"
  },
